{
  "$schema": "./schema/settings.schema.json",

  "settings": {
    "DISABLE_PETS": {
      "id": "pets",
      "title": "rpg.settings.disable-pets.title",
      "onEnable": [
        "set-view-pets %player% false"
      ],
      "onDisable": [
        "set-view-pets %player% true"
      ]
    },
    "DISABLE_PLAYERS": {
      "id": "players",
      "title": "rpg.settings.disable-players.title",
      "onEnable": [
        "set-view-players %player% false"
      ],
      "onDisable": [
        "set-view-players %player% true"
      ]
    },
    "DISABLE_SOUNDS": {
      "id": "sounds",
      "title": "rpg.settings.disable-sounds.title"
    }
  }
}